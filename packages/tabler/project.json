{
  "projectType": "library",
  "root": "packages/tabler",
  "sourceRoot": "packages/tabler/src",
  "prefix": "ng",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/packages/tabler"],
      "options": {
        "project": "packages/tabler/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/tabler/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/tabler/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/tabler"],
      "options": {
        "jestConfig": "packages/tabler/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/tabler/src/**/*.ts",
          "packages/tabler/src/**/*.html"
        ]
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
